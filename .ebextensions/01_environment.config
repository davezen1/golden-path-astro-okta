option_settings:
  # Set environment variables for Okta configuration
  # These should be configured in the EB console or via EB CLI
  # They will be passed as build arguments to Docker
  aws:elasticbeanstalk:application:environment:
    PUBLIC_OKTA_DOMAIN: "your-okta-domain.okta.com"
    PUBLIC_OKTA_CLIENT_ID: "your-client-id"
    PUBLIC_OKTA_REDIRECT_URI: "https://your-eb-url.elasticbeanstalk.com"
